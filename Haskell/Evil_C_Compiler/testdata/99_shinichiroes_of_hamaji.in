#include <stdio.h>
int main() {
   int i=99;  
   while (i>1) {
      printf("%d shinichiroes of hamaji on the wall, ",i);
      printf("%d shinichiroes of hamaji.\n", i);
      printf("Take one down and pass it around, ");
      i-=1;
      if (i>1) {
         printf("%d shinichiroes of hamaji on the wall.\n\n",i);
      }
   }
   printf("1 shinichiro of hamaji on the wall.\n\n");
   printf("1 shinichiro of hamaji on the wall, 1 shinichiro of hamaji.\n");
   printf("Go to the store and buy some more, 99 shinichiroes of hamaji on the wall.\n");
   return 0;
}
